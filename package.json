    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "get-google-fonts": "^1.2.2",
    "jest": "^27.2.4",
    "named-regexp-groups": "^1.0.5",
    "prettier": "^2.4.1",
