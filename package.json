{
  "name": "react-bootswatch-theme-switcher",
  "version": "1.0.7",
  "description": "A React component to select and switch between Bootstrap (Bootswatch) Themes",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run get_themes && babel src --out-dir lib",
    "prepublish": "npm run build",
    "clean": "rimraf ./lib",
    "get_themes": "node ./download_themes.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raythree/react-bootswatch-theme-switcher"
  },
  "keywords": [
    "Bootstrap",
    "React",
    "theme"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/czack425/react-bootswatch-theme-switcher/issues"
  },
  "homepage": "https://github.com/czack425/react-bootswatch-theme-switcher#readme",
  "files": [
    "lib",
    "src",
    "index.d.ts"
  ],
  "peerDependencies": {
    "react": ">=16.0",
    "bootstrap": "~4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "csso": "^3.5.1",
    "download-file": "^0.1.5",
    "fs-extra": "^8.1.0",
    "named-regexp-groups": "^1.0.3",
    "prop-types": "^15.7.2",
    "rimraf": "^2.5.4",
    "sync-request": "^6.1.0"
  }
}
